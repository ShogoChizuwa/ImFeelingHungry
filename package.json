{
  "name": "imfeelinghungry",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "copy-assets": "cpx \"public/**/*\" dist/public",
    "build": "npm run clean && tsc src/index.ts --outDir dist --module commonjs --target es2020 --esModuleInterop --listEmittedFiles && npm run copy-assets",
    "postbuild": "echo '--- Listing dist directory after build ---' && ls -R dist",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "cpx": "^1.5.0",
    "prisma": "^5.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "ts-node --project prisma/tsconfig.json prisma/seed.ts"
  }
}
